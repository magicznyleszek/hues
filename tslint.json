{
  "defaultSeverity": "warning",
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "max-line-length": {
      "options": [80]
    },
    "trailing-comma": false,
    "object-literal-key-quotes": {
      "severity": "warning",
      "options": ["as-needed"]
    }
  }
}
